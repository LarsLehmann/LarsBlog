              <%= image_tag 'fb.jpg' %>

              </a>
             &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
            </div>
              
            </ul>
          </div><!--/.nav-collapse -->
        </div>
      </div>
    </div>

    <div class="container">
      <div class="row">
        <div class="span9 well" style="margin-left: -1px; padding-right: 7px; margin-right: -16px; background-color: #FBFAFF; padding-left: 7px; ">
          
          <img width ="940px" src =  "http://www.imgbox.de/users/public/images/d7LXUW7cVj.jpg">
          <%= debug flash %>
          <div style=" word-wrap: normal ;">

            
            <%= yield %>
            

          </div>
        </div><!--/span-->
        <div class="span3">
          <div  style="background-color: white;" >
           
              <%= yield :infos %>
              <% begin %>
                 <script charset="utf-8" src="http://widgets.twimg.com/j/2/widget.js"></script>
<div class="well">
<script> 
new TWTR.Widget({
  version: 2,
  type: 'profile',
  rpp: 4,
  interval: 30000,
  width: 'auto',
  height: 300,
  theme: {
    shell: {
      background: '#F5F5F5',
      color: 'black'
    },
    tweets: {
      background: '#F5F5F5',
      color: 'black',
      links: 'blue'
    }
  },
  features: {
    scrollbar: false,
    loop: false,
    live: true,
    behavior: 'all'
  }
}).render().setUser('LieutenantBand').start();
</script>
</div>
<div class="well">
<iframe width="auto" height="295" style="position: relative; left: 39px; display: block; width: 150px; height: 295px; text-align: center;" src="http://bandcamp.com/EmbeddedPlayer/v=2/album=232232556/size=tall/bgcol=F5F5F5/linkcol=4285BB/" allowtransparency="true" frameborder="0"><a href="http://lieutenants.bandcamp.com/album/demo">Demo by Lieutenant! Lieutenant!</a></iframe>
</div>



              <% rescue Exception=> message %>
              <% message %>
              <% end %>
          
          </div>  
          <div class="well">
            <%= yield :shows %><br />
             <% if can? :manage, @shows %>
              <%= link_to 'Neue Show', new_show_path %><br />
              <%= link_to 'LÃ¶schen / Editieren', shows_path %>
            <% end %>

          </div>  

          <% if user_signed_in? %>
          <div class="well">
            <ul class="unstyled">
              <%= yield :adminlinks %>
            </ul>
          </div>
          <% end %>
        </div>
      </div><!--/row--> 
    </div> <!-- /container --> 
          

      

    



