<h1>Listing shows</h1>

<table>
  <tr>
    <th>Location</th>
    <th>Datum</th>
    <th>Support</th>
    <th>Eintritt</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @shows.each do |show| %>
  <tr>
    <td><%= show.location %></td>
    <td><%= show.showdate %></td>
    <td><%= show.support %></td>
    <td><%= show.eintritt %></td>
    
    <td><%= link_to 'Edit', edit_show_path(show) %></td>
    <td><%= link_to 'Destroy', show, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

