<% set_meta_tags :noindex => true %>
# <meta name="robots" content="noindex" />
<% set_meta_tags :noindex => 'googlebot' %>
# <meta name="googlebot" content="noindex" />

<%= semantic_form_for @show do |f| %>
  <%= f.inputs do %>
    <%= f.input :location %>
  
  <%= datetime_select("show", :showdate, :start_year => 1995) %>
    <%= f.input :support %>
    <%= f.input :eintritt %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
